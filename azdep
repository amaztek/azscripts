az deployment group create \
  --name export-ds \
  --resource-group <source-rg> \
  --template-file kv-diagnostic-export.json \
  --parameters \
    keyVaultName="mykv" \
    eventHubName="kv-logs" \
    userAssignedIdentity="/subscriptions/<sub-id>/resourceGroups/<rg>/providers/Microsoft.ManagedIdentity/userAssignedIdentities/umi-export-logs"
